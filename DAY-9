#include <stdio.h>
#include <string.h>

void reverseString(char s[], int n) {
    int start = 0, end = n - 1;

    while (start < end) {
        char temp = s[start];
        s[start] = s[end];
        s[end] = temp;

        start++;
        end--;
    }
}

int main() {
    char s[100000];

    scanf("%s", s);

    int n = strlen(s);

    reverseString(s, n);

    printf("%s", s);

    return 0;
}

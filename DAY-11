view problem:
#include <stdio.h>

int main() {
    int n1, n2;

    scanf("%d", &n1);
    int nums1[n1];
    for(int i = 0; i < n1; i++) scanf("%d", &nums1[i]);

    scanf("%d", &n2);
    int nums2[n2];
    for(int i = 0; i < n2; i++) scanf("%d", &nums2[i]);

    int freq[1001] = {0};   // assuming values â‰¤ 1000

    // Count frequency of nums1
    for(int i = 0; i < n1; i++) {
        freq[nums1[i]]++;
    }

    // Find intersection
    for(int i = 0; i < n2; i++) {
        if(freq[nums2[i]] > 0) {
            printf("%d ", nums2[i]);
            freq[nums2[i]]--;
        }
    }

    return 0;
}
solve problem:
#include <stdio.h>

int main() {
    int m, n;
    scanf("%d %d", &m, &n);

    int A[m][n];

    // Input matrix
    for(int i = 0; i < m; i++) {
        for(int j = 0; j < n; j++) {
            scanf("%d", &A[i][j]);
        }
    }

    // Print transpose
    for(int j = 0; j < n; j++) {
        for(int i = 0; i < m; i++) {
            printf("%d ", A[i][j]);
        }
        printf("\n");
    }

    return 0;
}

#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);   // n = expected max number

    int arr[n - 1];

    // Input array
    for (int i = 0; i < n - 1; i++) {
        scanf("%d", &arr[i]);
    }

    // Calculate expected sum
    int total = n * (n + 1) / 2;

    // Calculate array sum
    int sum = 0;
    for (int i = 0; i < n - 1; i++) {
        sum += arr[i];
    }

    // Missing number
    printf("%d", total - sum);

    return 0;
}

#include <stdio.h>

int main() {
    int m = 3, n = 3;
    int nums1[6] = {1, 2, 3, 0, 0, 0};
    int nums2[3] = {2, 5, 6};
    int temp[6];

    int i = 0, j = 0, k = 0;

    // Merge into temp array
    while (i < m && j < n) {
        if (nums1[i] < nums2[j])
            temp[k++] = nums1[i++];
        else
            temp[k++] = nums2[j++];
    }

    // Copy remaining elements
    while (i < m)
        temp[k++] = nums1[i++];

    while (j < n)
        temp[k++] = nums2[j++];

    // Copy back to nums1
    for (i = 0; i < m + n; i++)
        nums1[i] = temp[i];

    // Print result
    printf("Merged array:\n");
    for (i = 0; i < m + n; i++)
        printf("%d ", nums1[i]);

    return 0;
}
